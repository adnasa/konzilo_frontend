!!!
html(class="no-js", ng-app="konzilo.main")
  head
    meta(charset="utf-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge,chrome=1")
    title {{title}} - Konzilo
    meta(name="viewport", content="width=device-width")
    link(rel="stylesheet", href="css/main.css")
    for script in scripts
      script(src=script)
  body
    .header
      .content
        include navbar.jade
    div(ng-view)
